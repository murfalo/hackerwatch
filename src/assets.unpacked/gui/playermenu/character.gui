					<rect offset="0 18" width="3" height="28" spriteset="gui/variable/3pxbar_vert.sval" />
					<rect offset="3 18" width="3" height="28" spriteset="gui/variable/3pxbar_vert.sval" />

					<!-- Potion -->
					<sprite src="big-frame" offset="6 18">
						<sprite id="potion" src="potion-4" offset="2 2" canfocus="true">
							<dotbar id="potion-bar" offset="0 21" width="24" height="3" />
						</sprite>
					</sprite>

					<rect offset="34 18" width="3" height="28" spriteset="gui/variable/3pxbar_vert.sval" />
					<rect offset="37 18" width="8" height="28" spriteset="gui/variable/headline_borders.sval" />
					<rect offset="45 18" width="3" height="28" spriteset="gui/variable/3pxbar_vert.sval" />

					<!-- Skills -->
					<sprite id="skill-0" src="big-frame" offset="48 18"><sprite id="icon" offset="2 2" canfocus="true" /></sprite>
					<sprite id="skill-1" src="big-frame" offset="76 18"><sprite id="icon" offset="2 2" canfocus="true" /></sprite>
					<sprite id="skill-2" src="big-frame" offset="104 18"><sprite id="icon" offset="2 2" canfocus="true" /></sprite>
					<sprite id="skill-3" src="big-frame" offset="132 18"><sprite id="icon" offset="2 2" canfocus="true" /></sprite>
					<sprite id="skill-4" src="big-frame" offset="160 18"><sprite id="icon" offset="2 2" canfocus="true" /></sprite>
					<sprite id="skill-5" src="big-frame" offset="188 18"><sprite id="icon" offset="2 2" canfocus="true" /></sprite>
					<sprite id="skill-6" src="big-frame" offset="216 18"><sprite id="icon" offset="2 2" canfocus="true" /></sprite>

					<rect offset="244 18" width="3" height="28" spriteset="gui/variable/3pxbar_vert.sval" />
				</rect>

				<!-- Inventory -->
				<rect offset="247 0" width="81" height="202">
					<rect width="3" height="194" spriteset="gui/variable/3pxbar_vert.sval" />
					<rect offset="3 0" width="3" height="194" spriteset="gui/variable/3pxbar_vert.sval" />
					<rect offset="78 0" width="3" height="194" spriteset="gui/variable/3pxbar_vert.sval" />

					<rect offset="6 0" width="72" height="18" spriteset="gui/variable/headline_borders.sval">
						<text id="items-text" font="gui/fonts/arial11_bold.fnt" anchor="0.5 0.5" text=".playermenu.character.items" />
					</rect>

					<rect offset="0 194" width="81" height="8" spriteset="gui/variable/headline_borders.sval" />

					<inventoryitem id="inventory-template" width="16" height="16" visible="false" />
					<inventory id="inventory" offset="6 18" width="64" height="176" scroll-backdrop-sprite="inventory-item-frames" flow="hboxwrapped" />
					<scrollbar forid="inventory" spriteset="scrollbar" outside="true" buttons-size="14" trough-offset="18" handle-border="2" />
				</rect>
			</rect>
		</group>
