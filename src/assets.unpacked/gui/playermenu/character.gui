	<doc>
		<group>
%include "gui/playermenu/charheader.inc"

			<!-- Lower contents when dead -->
			<rect id="player-dead" offset="0 31" width="328" height="202" visible="false" color="#202a26FF" shadow="#0c120fFF" shadowsize="2">
				<text id="player-dead-text" font="gui/fonts/arial11.fnt" anchor="0.5 0.5" />
			</rect>

			<!-- Lower contents -->
			<rect id="player-info" offset="0 31" width="328" height="202">
				<!-- Character info -->
				<rect width="247" height="156">
					<!-- Vertical separators -->
					<rect offset="122 0" width="3" height="49" spriteset="gui/variable/3pxbar_vert.sval" />
					<rect offset="122 104" width="3" height="49" spriteset="gui/variable/3pxbar_vert.sval" />

					<!-- Horizontal separators -->
					<rect offset="0 49" width="247" height="3" spriteset="gui/variable/3pxbar_hor.sval" />
					<rect offset="0 101" width="247" height="3" spriteset="gui/variable/3pxbar_hor.sval" />
					<rect offset="0 153" width="247" height="3" spriteset="gui/variable/3pxbar_hor.sval" />

					<!-- Vitality -->
					<rect width="122" height="49" offset="0 0">
						<rect width="122" height="18" spriteset="gui/variable/headline_borders.sval">
							<text font="gui/fonts/arial11_bold.fnt" anchor="0.5 0.5" text=".playermenu.character.vitality" />
						</rect>

						<rect offset="0 18" width="122" height="31" color="#202a26FF" shadow="#0c120fFF" shadowsize="2">
							<group inner="true" offset="12 0">
								<sprite src="icon-health" offset="0 2" />
								<text id="info_health" font="gui/fonts/arial11.fnt" offset="18 0" />
							</group>

							<group inner="true" offset="12 15">
								<sprite src="icon-health-regen" offset="0 2" />
								<text id="info_health_regen" font="gui/fonts/arial11.fnt" offset="18 0" />
							</group>

							<group inner="true" offset="72 0">
								<sprite src="icon-mana" offset="0 2" />
								<text id="info_mana" font="gui/fonts/arial11.fnt" offset="15 0" />
							</group>

							<group inner="true" offset="72 15">
								<sprite src="icon-mana-regen" offset="0 2" />
								<text id="info_mana_regen" font="gui/fonts/arial11.fnt" offset="15 0" />
							</group>
						</rect>
					</rect>

					<!-- Speed / exp -->
					<rect width="122" height="49" offset="125 0">
						<rect width="122" height="18" spriteset="gui/variable/headline_borders.sval">
							<text font="gui/fonts/arial11_bold.fnt" anchor="0.5 0.5" text=".playermenu.character.speedexp" />
						</rect>

						<rect offset="0 18" width="122" height="31" color="#202a26FF" shadow="#0c120fFF" shadowsize="2">
							<group inner="true" offset="12 8">
								<sprite src="icon-speed" offset="0 2" />
								<text id="info_move_speed" font="gui/fonts/arial11.fnt" offset="18 0" />
							</group>

							<group inner="true" offset="64 8">
								<sprite src="icon-exp" offset="0 2" />
								<text id="info_exp" font="gui/fonts/arial11.fnt" offset="16 0" />
							</group>
						</rect>
					</rect>

					<!-- Combat -->
					<rect width="247" height="49" offset="0 52">
						<rect width="247" height="18" spriteset="gui/variable/headline_borders.sval">
							<text font="gui/fonts/arial11_bold.fnt" anchor="0.5 0.5" text=".playermenu.character.combat" />
