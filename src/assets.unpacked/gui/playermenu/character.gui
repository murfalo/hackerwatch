					<rect width="122" height="49" offset="0 104">
						<rect width="122" height="18" spriteset="gui/variable/headline_borders.sval">
							<text font="gui/fonts/arial11_bold.fnt" anchor="0.5 0.5" text=".playermenu.character.resources" />
						</rect>

						<rect offset="0 18" width="122" height="31" color="#202a26FF" shadow="#0c120fFF" shadowsize="2">
							<group inner="true" offset="6 0">
								<sprite src="icon-gold" offset="0 3" />
								<text id="info_gold" font="gui/fonts/arial11.fnt" offset="16 0" />
							</group>

							<group inner="true" offset="4 16">
								<sprite src="icon-gold-town" offset="0 1" />
