<effect looping="false">
	
	%//Particles
	
	<sprite origin="3 3" looping="false" texture="./dagger_projectile.png" material="system/default.mats:default-noxray"
		num-sprites="2"

		pos-x="t*t*cos(rand(u+13*i)*2*pi) * 5 * pow(rand(u+37*i),0.5) - cos(rand(u+13*i)*2*pi) * 5* pow(rand(u+37*i),0.5) - t*cos(rand(u+13*i)*2*pi) * 5 * pow(rand(u+37*i),0.5)"
        pos-y="t*t*sin(rand(u+13*i)*2*pi) * 5 * pow(rand(u+37*i),0.5) - sin(rand(u+13*i)*2*pi) * 5* pow(rand(u+37*i),0.5) - t*sin(rand(u+13*i)*2*pi) * 5 * pow(rand(u+37*i),0.5) - sin(t*t*t*t*t)*rand(i)*15"
		
		color-a="1 - t"
	>
        
		<frame time="100">6 32 6 6</frame>
		<frame time="100">12 32 6 6</frame>
		<frame time="200">18 32 6 6</frame>
		<frame time="200">24 32 6 6</frame>
		<frame time="200">30 32 6 6</frame>
		<frame time="100">36 32 6 6</frame>
		<frame time="100">42 32 6 6</frame>
	</sprite>
	
	
	<sprite origin="3 3" looping="false" texture="./dagger_projectile.png" material="system/default.mats:glow"
		num-sprites="2"

		pos-x="t*t*cos(rand(u+13*i)*2*pi) * 5 * pow(rand(u+37*i),0.5) - cos(rand(u+13*i)*2*pi) * 5* pow(rand(u+37*i),0.5) - t*cos(rand(u+13*i)*2*pi) * 5 * pow(rand(u+37*i),0.5)"
        pos-y="t*t*sin(rand(u+13*i)*2*pi) * 5 * pow(rand(u+37*i),0.5) - sin(rand(u+13*i)*2*pi) * 5* pow(rand(u+37*i),0.5) - t*sin(rand(u+13*i)*2*pi) * 5 * pow(rand(u+37*i),0.5) - sin(t*t*t*t*t)*rand(i)*15"
		
		color-a="1 - t"
	>
        
		<frame time="100">6 32 6 6</frame>
		<frame time="100">12 32 6 6</frame>
		<frame time="200">18 32 6 6</frame>
		<frame time="200">24 32 6 6</frame>
		<frame time="200">30 32 6 6</frame>
		<frame time="100">36 32 6 6</frame>
		<frame time="100">42 32 6 6</frame>
	</sprite>
	
