<effect looping="true" 
%if !GFX_VFX_LOW
wpos-track="50"
%endif
>
	<param name="angle" default="0" />
	<param name="height" default="0" />


	
	%// Rocket
	<sprite looping="true" origin="3 4" texture="./tower_rock.png" material="system/default.mats:projectile"
	
		pos-x="0"
		pos-y="-height"
		>
		
		<frame time="50">0 0 6 8</frame>
		<frame time="50">6 0 6 8</frame>
		<frame time="50">0 8 6 8</frame>
		<frame time="50">6 8 6 8</frame>
	</sprite>
</effect>