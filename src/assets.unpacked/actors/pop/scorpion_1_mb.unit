					<int name="dist">10</int>
					<string name="projectile">actors/pop/projectiles/scorpion_groundspikes.unit</string>
				</dict>
			</dict>

			%// NG+ Lurker
			<dict>
				<array name="conditional">
					<string>ngp</string><string>ge</string><int>1</int>
				</array>
			
				<string name="class">CompositeActorBurstSkill</string>
				<string name="anim">attack-fast 8</string>
				<string name="snd">event:/enemy/scorpion_mb/lurker</string>
				<int name="burst">4</int>
				
				<bool name="good-aim">true</bool>
				<bool name="must-see">true</bool>
