					<string name="class">ShootProjectile</string>
					<int name="dist">10</int>
					<int name="spread">15</int>
					<string name="projectile">actors/pop/projectiles/scorpion_groundspikes.unit</string>
				</dict>
			</dict>

			%// Charge
			<dict>
				<string name="class">ChargeStrike</string>
				<string name="anim">charge 8</string>
				<string name="anim-stop">pinch-fast 8</string>
				
